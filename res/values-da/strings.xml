    <string name="wimax_status_disconnecting">Afslutter forbindelse\u2026</string>
    <!-- Status message of wimax when it is disconnecting from a network -->
    <string name="wimax_status_disconnecting_from">Afsluttede forbindelse til <xliff:g id="network_name">%1$s</xliff:g>\u2026</string>

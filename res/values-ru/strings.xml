    <string name="wimax_status_disconnecting">Отключение\u2026</string>
    <!-- Status message of wimax when it is disconnecting from a network -->
    <string name="wimax_status_disconnecting_from">Отключение от <xliff:g id="network_name">%1$s</xliff:g>\u2026</string>
    <!-- Status message of wimax when it is disconnected -->
    <string name="wimax_status_disconnected">Отключено</string>

    <!-- About phone, status item value if the actual value is not available. -->
    <string name="status_unavailable">Недоступно</string>

